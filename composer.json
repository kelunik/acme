{
    "name": "kelunik/acme",
    "description": "ACME library written in PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "keywords": [
        "ACME",
        "letsencrypt",
        "certificate",
        "https",
        "encryption",
        "ssl",
        "tls"
    ],
    "autoload": {
        "psr-4": {
            "Kelunik\\Acme\\": "src"
        },
        "files": [
            "src/functions.php",
            "src/Protocol/functions.php"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ext-openssl": "*",
        "amphp/amp": "^3",
        "amphp/file": "^3",
        "amphp/http-client": "^5",
        "sabre/uri": "^1",
        "psr/log": "^1 || ^2 || ^3",
        "kelunik/certificate": "^1.1",
        "beberlei/assert": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "amphp/phpunit-util": "^3",
        "amphp/php-cs-fixer-config": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1-dev"
        }
    }
}
